{
  "name": "tintnaingwin/myanfont",
  "description": "Laravel Zawgyi <=> Unicode Package",
  "keywords": [
    "laravel",
    "zawgyi",
    "unicode",
    "myanmar-font",
    "myanfont"
  ],
  "homepage": "https://github.com/tintnaingwinn/MyanFont",
  "license": "MIT",
  "authors": [
    {
      "name": "Tint Naing Win",
      "email": "tnwdeveloper@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "googlei18n/myanmar-tools": "^1.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Tintnaingwin\\MyanFont\\": "src/"
    },
    "files": [
      "src/Rabbit.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "test/DataTestHelper.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "aliases": {
        "MyanFont": "Tintnaingwin\\MyanFont\\Facades\\MyanFont"
      }
    }
  }
}
