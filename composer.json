{
  "name": "tintnaingwin/myanfont",
  "description": "Laravel Zawgyi <=> Unicode Package",
  "keywords": [
    "laravel",
    "zawgyi",
    "unicode",
    "myanmar-font",
    "myanfont"
  ],
  "homepage": "https://github.com/tintnaingwinn/MyanFont",
  "license": "MIT",
  "authors": [
    {
      "name": "Tint Naing Win",
      "email": "tnwdeveloper@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "googlei18n/myanmar-tools": "^1.1",
    "illuminate/support": "~5.5|~5.6|~5.7|~5.8|^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.5|^4.0",
    "phpunit/phpunit": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Tintnaingwin\\MyanFont\\": "src/"
    },
    "files": [
      "src/Rabbit.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tintnaingwin\\MyanFont\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "aliases": {
        "MyanFont": "Tintnaingwin\\MyanFont\\Facades\\MyanFont"
      }
    }
  }
}
